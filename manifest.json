{
  "author": "Smarthard",
  "content_scripts": [
    {
      "js": [
        "main.js"
      ],
      "matches": [
        "*://ficbook.net/authors/*"
      ],
      "run_at": "document_idle"
    }
  ],
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "manifest_version": 2,
  "name": "Ficbook Downloader",
  "description": "",
  "icons": {
    "48": "assets/icon-48x48.png"
  },
  "permissions": [
    "tabs",
    "*://ficbook.net/*"
  ],
  "web_accessible_resources": [
    "assets/*.png",
    "bin/*.js",
    "index.html",
    "styles.css",
    "*.js"
  ],
  "version": "1.0.0"
}
